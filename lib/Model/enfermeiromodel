import 'package:meuapppdv/Model/usermodel.dart';

class Enfermeiromodel extends UsuarioModel {
  final String coren;



  Enfermeiromodel({
    
    required super.nome,
    required super.email,
    required super.senha,
    required super.dtNascimento,
    required super.telefone,
    required this.coren,
  });


  bool crmvalido() {
    return crm.length == 7;
  }

}